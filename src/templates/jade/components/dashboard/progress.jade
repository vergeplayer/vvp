.vvp-progress(role="progressbar", aria-valuenow="0", aria-valuemax="100", aria-valuemin="0", aria-label="播放进度条", aria-valuetext="当前时间 0:01")
    .vvp-progress-track
    .vvp-progress-buffer(id="buffer" style="width:0px;")
    .vvp-progress-played(id="played" style="width:0px;")
    .vvp-progress-handle(id="handle")
    .vvp-progress-seek(role="presentation", value="")
